// Image Single
// ============================================================

@s-img-radius: @radius;


.s-img {
  display: block;
  &.-inline {
    display: inline-block;
  }
  &.-radius {
    border-radius: @s-img-radius;
  }
  &.-round {
    border-radius: 100%;
  }
  &.-fluid {
    width: 100%;
  }
}
